{
  "extends": ["solhint:recommended"],
  "rules": {
    "compiler-version": ["error", "0.8.23"],
    "max-line-length": ["warn", 120],
    "no-inline-assembly": "off",
    "func-visibility": ["error", {"ignoreConstructors": true}],
    "custom-errors": "off",
    "immutable-vars-naming": "off"
  }
}
